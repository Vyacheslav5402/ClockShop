@model CustomClockModel

<div class="row bg-white">
    <div class="col-sm py-3 py-md-3 px-3">
        <h3 class=" px-0 px-md-4">Custom clock</h3>
        <ul class="px-0 px-md-4 list-unstyled">
            <li class="py-1">Watchband - @Model.Watchband</li>
            <li class="py-1">ClockFase - @Model.ClockHands</li>
            <li class="py-1">ClockHands - @Model.ClockFase</li>
            <li class="py-1">ClockCase - @Model.ClockCase</li>
        </ul>
    </div>
</div>
