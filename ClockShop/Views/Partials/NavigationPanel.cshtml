<nav aria-label="Page navigation example">
    <ul class="pagination">
        <li class="page-item">
        </li>
        @for (int i = 1; i < Model.AmountOfPage; i++)
        {
            <li class="page-item"><a class="page-link" href="javascript:void(0);" onclick="pageNavigation(@i)" role="button">@i</a></li>
        }
        <li class="page-item">
        </li>
    </ul>
</nav>